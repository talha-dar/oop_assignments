#include <iostream>
#include <cstring>
#include "MyString.h"
#include "MyString.cpp"
using namespace std;

int main()
{

	MyString obj1;
	for (char c = 'A'; c <= 'Z'; c++)
		obj1.add(c);
	cout << "\nPrinting obj1: ";
	print(obj1);
	MyString obj2(obj1, 4, 10);
	cout << "\nPrint obj2 for obj1(4,10): ";
	print(obj2);

	cout << "\nPrinting obj2[1]=@: ";
	obj2[1] = '@';
	print(obj2);

	const MyString obj3(obj2);
	//obj3[3]='7';            // uncomment this line and check what it say
	MyString obj4;
	obj4=obj1+obj2;
	cout<<"\nAfter object addition: ";
	print(obj4);

	MyString obj5;
	obj5=obj4;
	cout<<"\nAfter assignment operator: ";
	print(obj5);
	return 0;
}
